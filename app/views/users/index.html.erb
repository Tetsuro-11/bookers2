<% @users.each do |users| %>
<div>
  <table class='table table-hover table-inverse'>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th></th>
        <th colspan="3"></th>
      </tr>
    </thead>
    
    <tbody>
        <% @users.each do |user|%>
        <tr>
          <td>
            <%= user.name %>
          </td>
          <td><%= link_to user.name, user_path(user) %></td>
          <td><%= link_to "Show", user_path(user) %></td>
        </tr>
        <% end %>
    </tbody>
  </table>
</div>
<% end %>

